{
  "name": "create-react-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "component": "node scripts/component.js",
    "redux": "node scripts/redux.js",
    "get": "node scripts/type.js"
  },
  "devDependencies": {
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "^5.1.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "pong-react-scripts": "^3.4.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.28.0",
    "@types/crypto-js": "^3.1.46",
    "add": "^2.0.6",
    "crypto-js": "^4.0.0",
    "rc-field-form": "^1.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "rxjs": "^6.5.5",
    "typeface-muli": "^1.1.3",
    "use-rx-hooks": "^1.4.3-beta.1",
    "yarn": "^1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --rule 'no-console: [\"error\", { allow: [\"warn\", \"error\"] }]' --max-warnings=0",
      "prettier --ignore-path .eslintignore --write",
      "git add"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write",
      "git add"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write",
      "git add"
    ],
    "*.{yml,md}": [
      "prettier --ignore-path .eslintignore --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react/self-closing-comp": "warn"
    }
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
